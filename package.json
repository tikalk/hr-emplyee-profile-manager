{
  "name": "employee-manager",
  "version": "0.1.0",
  "main": "main.js",
  "scripts": {
    "start": "HOT=true ./node_modules/electron/dist/Electron.app/Contents/MacOS/Electron .",
    "watch": "./node_modules/.bin/webpack-dev-server",
    "build": "./node_modules/.bin/webpack",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild",
    "build-all": "npm run build && electron-packager ./ employee-manager --all --overwrite",
    "build-win": "npm run build && electron-packager ./ employee-manager --paltform=win32 --arch=ia32 --icon=./icon.png --overwrite",
    "build-mac": "npm run build && electron-packager ./ employee-manager --ignore=.idea/* --ignore=node_modules/* --platform=darwin --arch=x64 --icon=./icon.png.icns --overwrite --prune",
    "build-linux": "npm run build && electron-packager ./ employee-manager --platform=linux --arch=x64 --icon=./icon.png --asar",
    "lint": "./node_modules/.bin/eslint src"
  },
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-html-attrs": "2.0.0",
    "babel-plugin-transform-class-properties": "6.3.13",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "css-loader": "0.25.0",
    "electron": "^1.4.15",
    "electron-packager": "8.1.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "2.0.0",
    "eslint-plugin-jsx-a11y": "2.2.1",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "0.9.0",
    "img-loader": "1.3.1",
    "less": "2.5.1",
    "less-loader": "2.2.0",
    "node-libs-browser": "1.0.0",
    "style-loader": "0.13.1",
    "webpack": "1.9.12",
    "webpack-dev-server": "3.1.11"
  },
  "dependencies": {
    "bootstrap": "3.4.0",
    "classnames": "2.2.5",
    "cloudinary": "1.4.3",
    "github-api": "2.3.0",
    "jquery": "3.1.0",
    "js-yaml": "3.6.1",
    "json-loader": "0.5.4",
    "lodash": "4.17.5",
    "react": "15.3.0",
    "react-autobind": "1.0.6",
    "react-dom": "15.3.0",
    "react-dropzone": "3.6.0",
    "react-markdown": "2.4.2",
    "request": "2.74.0"
  }
}
